@model Cliente

<form asp-action="Create">
	<label asp-for="Nome">Nome</label>
	<input asp-for="Nome" placeholder="Digite seu nome" required />

	<label asp-for="DataNascimento">Data de Nascimento </label>
	<input asp-for="DataNascimento" required />

	<label asp-for="Salario">Salário</label>
	<input asp-for="Salario" placeholder="Digite seu salário" required />

	<input class="btn btn-primary" type="submit" value="Salvar" />
</form>

